generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model RegistrasiInstansi {
  id                       Int                      @id @default(autoincrement())
  statusRegistrasiInstansi StatusRegistrasiInstansi @default(BELUM)
  nama                     String
  email                    String
  nomorTelepon             String
  desaKelurahan            String
  kecamatan                String
  kabupatenKota            String
  password                 String
  alamat                   String
  createdAt                DateTime                 @default(now())
  updatedAt                DateTime                 @updatedAt
}

enum StatusRegistrasiInstansi {
  BELUM
  DISETUJUI
}
